package mypack;

public class MyJavaClass {
	public static void main(String[] args) {
	System.out.print("Hello World!!");	
	System.out.println();
	/*
	 * 
	 */
	int x = 100;
	float f1 = 99.9f;
	System.out.println("f1 = " + f1);
	double d1 = 22.34d;
	System.out.println("d1 = " + d1);
	
	/*
	 *  Type casting
	 *  
	 */
	float m = x;
	System.out.println("m = " + m); // implicit casting -smaller to larger
	
	double r = 78.456;
	int h = (int)r;
	
	System.out.println("h = " + h); // explicit casting -larger to smaller
	
	char c ='a';
	int s = c;
	System.out.println("s = " + s);
	
	
	
	
	}
	

}
